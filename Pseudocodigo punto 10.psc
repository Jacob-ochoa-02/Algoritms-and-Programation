Algoritmo Programa_de_becas
	Escribir "Nota del estudiante"
	Leer Nota_del_estudiante
	Escribir "Beca del estudiante"
	Leer Beca
	Si Nota_del_estudiante>=4.5 Entonces
		Tendra_beca_completa_de=800000
	SiNo
		Tendra_solo_ayuda_de=500000
	Fin Si
	Escribir "El estudiante tiene beca de: ",beca
FinAlgoritmo
